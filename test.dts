/dts-v1/;

/ {
    #address-cells = <1>;
    #size-cells = <1>;
    compatible = "test,minimal-system";

    cpus {
        #address-cells = <1>;
        #size-cells = <0>;

        cpu0: cpu@0 {
            device_type = "cpu";
            reg = <0>;
        };

        cpu1: cpu@1 {
            device_type = "cpu";
            reg = <1>;
        };
    };

    memory@0 {
        device_type = "memory";
        reg = <0x00000000 0x1000000>;
    };
};
